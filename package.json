{
  "name": "violet-void-theme_chatgpt",
  "version": "1.0.0",
  "description": "Dark Violet-Void theme for ChatGPT.",
  "author": "aaronedev",
  "license": "MIT",
  "homepage": "https://github.com/aaronedev/violet-void-theme_chatgpt",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronedev/violet-void-theme.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "watch": "stylus -w src/main.styl -o dist/main.css",
    "lint": "stylint src",
    "lint:fix": "stylint src",
    "format": "prettier --write \"**/*.{js,json,styl}\""
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-plugin-stylus": "^1.0.0",
    "stylint": "^2.0.0",
    "stylus": "^0.64.0"
  }
}
